{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "title": "World GDP per capita 2020",
    "projection": {"type": "equalEarth"},
    "data": {
        "url": "https://raw.githubusercontent.com/1099238610/global-map-vis/main/json/global.topo.json?token=AQLXTUEYYHHFRNSLHYPPRJDBJW2OA",
        "format": {"type": "topojson", "feature": "custom.geo"}
      },
    "transform": [{
          "lookup": "properties.name",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/1099238610/global-map-vis/main/data/GDP%20per%20capita.csv?token=AQLXTUEMTRYKYT665M4MYFTBJW23O"
            },
            "key": "Country",
            "fields": ["GDP_Capita"]
          }
        }
      ],
    "mark": {"type": "geoshape"}
}